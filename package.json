{
  "name": "clean-code-node-api",
  "version": "3.0.1",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "author": "Renato Pereira",
  "license": "ISC",
  "keywords": [],
  "repository": "https://github.com/renatops1991/clean-code-api",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "start:dev": "npm run build && docker-compose up -d",
    "stop:dev": "docker-compose down",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "prune": "ts-prune",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@shelf/jest-mongodb": "^4.1.0",
    "@swc/core": "^1.3.3",
    "@swc/jest": "^0.2.22",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-prune": "^0.10.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/utils": "^8.12.0",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.10.0",
    "nodemon": "^2.0.20",
    "swagger-ui-express": "^4.5.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}